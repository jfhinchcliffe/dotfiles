# Set VS Code as the default editor
editor = '/Applications/Visual Studio Code.app/Contents/MacOS/Electron' -w

# Aliases
source ~/dotfiles/aliases/general
source ~/dotfiles/aliases/git
source ~/dotfiles/aliases/ruby_rails
source ~/dotfiles/aliases/work

# Terminal look and feel
source ~/dotfiles/terminal/lookandfeel

# Git autocompletion script
source ~/dotfiles/scripts/gitcompletion.bash

# Ruby versioning using Chruby
source /usr/local/opt/chruby/share/chruby/chruby.sh

